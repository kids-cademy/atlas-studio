<?xml version="1.0" encoding="UTF-8"?>
<project>
	<author>Iulian Rotaru</author>
	<name>atlas-studio</name>
	<display>Atlas Studio</display>
	<description>Development environment for Atlas collections and releases.</description>
	<locale>en</locale>

	<site-dir>build/site</site-dir>
	<naming-strategy>XMLNS</naming-strategy>
	<script-dependency-strategy>DESCRIPTOR</script-dependency-strategy>

	<metas>
		<meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
	</metas>

	<fonts>
		<font>http://fonts.googleapis.com/css?family=Montserrat&amp;display=swap</font>
		<font>http://fonts.googleapis.com/css?family=Open+Sans</font>
	</fonts>
	
	<scripts>
		<script src="https://cdn.jsdelivr.net/remarkable/1.7.1/remarkable.min.js"></script>
		<script src="https://unpkg.com/turndown/dist/turndown.js"></script>
	</scripts>
		
</project>
